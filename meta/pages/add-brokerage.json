                        }
                      ],
                      "name": "row0",
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              },
                              "text": "Cancel",
                              "type": "button"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "type": "row"
                    }
                  ],
                  "data": "${brokerage}",
                  "events": {
                    "submit": [
                      {
                        "name": "bDuplicateNumber",
                        "type": "set",
                        "value": "false",
                        "valueType": "bool"
                      },
                      {
                        "name": "RECODb.getBrokerages",
                        "parameters": [
                          {
                            "name": "$filter",
                            "operator": "and",
                            "value": [
                              {
                                "operation": "eq",
                                "property": "Name",
                                "type": "string",
                                "value": "${brokerage.Name}"
                              },
                              {
                                "operation": "ne",
                                "property": "BrokerageID",
                                "type": "integer",
                                "value": "${brokerage.BrokerageID}"
                              }
                            ]
                          }
                        ],
                        "then": [
                          {
                            "name": "bDuplicateName",
                            "type": "set",
                            "value": "${result.Count() > 0}",
                            "valueType": "bool"
                          }
                        ],
                        "type": "invoke"
                      },
                      {
                        "condition": "${Globals.generalsettings.ApplicationName == \"XLG CMS\"}",
                        "name": "RECODb.getBrokerages",
                        "parameters": [
                          {
                            "name": "$filter",
                            "operator": "and",
                            "value": [
                              {
                                "operation": "eq",
                                "property": "RegistrantNo",
                                "type": "string",
                                "value": "${brokerage.RegistrantNo}"
                              },
                              {
                                "operation": "ne",
                                "property": "BrokerageID",
                                "type": "integer",
                                "value": "${brokerage.BrokerageID}"
                              }
                            ]
                          }
                        ],
                        "then": [
                          {
                            "name": "bDuplicateNumber",
                            "type": "set",
                            "value": "${result.Count() > 0}",
                            "valueType": "bool"
                          }
                        ],
                        "type": "invoke"
                      },
                      {
                        "condition": "${!bDuplicateName && !bDuplicateNumber}",
                        "error": [
                          {
                            "detail": "${result.error.message}",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "RECODb.createBrokerage",
                        "parameters": [
                          {
                            "name": "Brokerage",
                            "value": "${brokerage}"
                          }
                        ],
                        "then": [
                          {
                            "result": "${brokerage}",
                            "type": "close-popup"
                          }
                        ],
                        "type": "invoke"
                      },
                      {
                        "condition": "${bDuplicateName}",
                        "detail": "Brokerage Name Already Exists",
                        "severity": "warn",
                        "summary": "Unable to Save",
                        "type": "notify"
                      },
                      {
                        "condition": "${bDuplicateNumber}",
                        "detail": "Brokerage Number Already In System",
                        "severity": "warn",
                        "summary": "Unable to Save Brokerage",
                        "type": "notify"
                      }
                    ]
                  },
                  "itemType": "RecoCms6.Models.RecoDb.Brokerage",
                  "name": "form0",
                  "type": "template-form",
                  "visible": "${brokerage != null}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "RECODb.getParameterDetails",
        "parameters": [
          {
            "name": "$filter",
            "operator": "or",
            "value": [
              {
                "operation": "eq",
                "property": "ParamTypeDesc",
                "type": "string",
                "value": "\"Province\""
              },
              {
                "operation": "eq",
                "property": "ParamTypeDesc",
                "type": "string",
                "value": "\"Method of Communication\""
              },
              {
                "operation": "eq",
                "property": "ParamTypeDesc",
                "type": "string",
                "value": "\"Brokerage Role\""
              }
            ]
          },
          {
            "name": "$orderby",
            "value": [
              {
                "direction": "asc",
                "property": "ParamDesc"
              }
            ]
          }
        ],
        "then": [
          {
            "name": "getCommunicationMethodsResult",
            "type": "set",
            "value": "${result.Where(p=>p.ParamTypeDesc==\"Method Of Communication\")}",
            "valueType": "IEnumerable<RecoCms6.Models.RecoDb.ParameterDetail>"
          },
          {
            "name": "getProvinceList",
            "type": "set",
            "value": "${result.Where(p=>p.ParamTypeDesc==\"Province\")}",
            "valueType": "IEnumerable<RecoCms6.Models.RecoDb.ParameterDetail>"
          },
          {
            "name": "getBrokerageRoleList",
            "type": "set",
            "value": "${result.Where(p=>p.ParamTypeDesc==\"Brokerage Role\")}",
            "valueType": "IEnumerable<RecoCms6.Models.RecoDb.ParameterDetail>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "RECODb.getAdministrators",
        "parameters": [],
        "then": [
          {
            "name": "getAdministratorsResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RecoCms6.Models.RecoDb.Administrator>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "brokerage",
        "type": "set",
        "value": "new RecoCms6.Models.RecoDb.Brokerage(){}",
        "valueType": "RecoCms6.Models.RecoDb.Brokerage"
      }
    ]
  },
  "layout": "Main",
  "name": "Add Brokerage"
}