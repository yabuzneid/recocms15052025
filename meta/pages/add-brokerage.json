                                {
                                  "components": [
                                    {
                                      "class": "col-md-12",
                                      "components": [
                                        {
                                          "events": {
                                            "click": [
                                              {
                                                "name": "InsertContactRow",
                                                "schema": {
                                                  "$ref": "#/definitions/Task",
                                                  "dotnetType": "System.Threading.Tasks.Task"
                                                },
                                                "type": "invoke-method"
                                              }
                                            ]
                                          },
                                          "icon": "add_circle_outline",
                                          "name": "buttonAddContact",
                                          "text": "Add New Contact",
                                          "type": "button"
                                        }
                                      ],
                                      "name": "col3",
                                      "type": "col"
                                    }
                                  ],
                                  "name": "row2",
                                  "type": "row"
                                },
                                {
                                  "attributes": [
                                    {
                                      "name": "ref",
                                      "value": "contactsGrid"
                                    }
                                  ],
                                  "columns": [
                                    {
                                      "property": "Name",
                                      "title": "Name"
                                    },
                                    {
                                      "property": "Email",
                                      "title": "Email"
                                    },
                                    {
                                      "property": "PhoneNum",
                                      "title": "Phone #"
                                    },
                                    {
                                      "property": "BrokerageRoleID",
                                      "template": [
                                        {
                                          "data": "${getBrokerageRoleList}",
                                          "name": "dropdown0",
                                          "textProperty": "ParamDesc",
                                          "type": "dropdown",
                                          "valueProperty": "ParameterID"
                                        }
                                      ],
                                      "title": "Role"
                                    }
                                  ],
                                  "count": "",
                                  "data": "${brokerage.BrokerageContacts}",
                                  "itemType": "RecoCms6.Models.RecoDb.BrokerageContact",
                                  "name": "contactsGrid",
                                  "type": "datagrid"
                                }
                              ],
                              "name": "fieldset0",
                              "text": "Additional Contacts",
                              "type": "fieldset",
                              "visible": false
                            }
                          ],
