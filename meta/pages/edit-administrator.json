                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Administrator",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "RECODb.updateAdministrator",
                        "parameters": [
                          {
                            "name": "ID",
                            "value": "${parameters.ID}"
                          },
                          {
                            "name": "Administrator",
                            "value": "${administrator}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${administrator}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "RecoCms6.Models.RecoDb.Administrator",
                  "data": "${administrator}",
                  "visible": "${administrator != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "RECODb.getAdministratorByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "administrator",
            "type": "set",
            "value": "${result}",
            "valueType": "RecoCms6.Models.RecoDb.Administrator"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "RECODb.getParameters",
        "parameters": [],
        "then": [
          {
            "name": "getParametersResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RecoCms6.Models.RecoDb.Parameter>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Administrator",
  "layout": "Main",
  "access": [
    "Authenticated"
  ]
}