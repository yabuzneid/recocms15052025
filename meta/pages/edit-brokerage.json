                  "data": "${brokerage}",
                  "events": {
                    "submit": [
                      {
                        "name": "bDuplicateRegNo",
                        "type": "set",
                        "value": "false",
                        "valueType": "bool"
                      },
                      {
                        "name": "bDuuplicateName",
                        "type": "set",
                        "value": "false",
                        "valueType": "bool"
                      },
                      {
                        "condition": "${Globals.generalsettings.ApplicationName == \"RECO CMS\" && origBrokerage.RegistrantNo != brokerage.RegistrantNo}",
                        "name": "RECODb.getBrokerages",
                        "parameters": [
                          {
                            "name": "$filter",
                            "operator": "and",
                            "value": [
                              {
                                "operation": "eq",
                                "property": "RegistrantNo",
                                "type": "string",
                                "value": "${brokerage.RegistrantNo}"
                              },
                              {
                                "operation": "ne",
                                "property": "BrokerageID",
                                "type": "integer",
                                "value": "${brokerage.BrokerageID}"
                              }
                            ]
                          }
                        ],
                        "then": [
                          {
                            "name": "bDuplicateRegNo",
                            "type": "set",
                            "value": "${result.Count() > 0}",
                            "valueType": "bool"
                          }
                        ],
                        "type": "invoke"
                      },
                      {
                        "condition": "${origBrokerage.Name != brokerage.Name}",
                        "name": "RECODb.getBrokerages",
                        "parameters": [
                          {
                            "name": "$filter",
                            "operator": "and",
                            "value": [
                              {
                                "operation": "eq",
                                "property": "Name",
                                "type": "string",
                                "value": "${brokerage.Name}"
                              },
                              {
                                "operation": "ne",
                                "property": "BrokerageID",
                                "type": "integer",
                                "value": "${brokerage.BrokerageID}"
                              }
                            ]
                          }
                        ],
                        "then": [
                          {
                            "name": "bDuplicateName",
                            "type": "set",
                            "value": "${result.Count() > 0}",
                            "valueType": "bool"
                          }
                        ],
                        "type": "invoke"
                      },
                      {
                        "condition": "${!bDuplicateRegNo && !bDuplicateName}",
                        "error": [
                          {
                            "detail": "Unable to update Brokerage",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "RECODb.updateBrokerage",
                        "parameters": [
                          {
                            "name": "BrokerageID",
                            "value": "${parameters.BrokerageID}"
                          },
                          {
                            "name": "Brokerage",
                            "value": "${brokerage}"
                          }
                        ],
                        "then": [
                          {
                            "result": "${brokerage}",
                            "type": "close-popup"
                          }
                        ],
                        "type": "invoke"
                      },
                      {
                        "condition": "${bDuplicateRegNo}",
                        "detail": "Registrant Number already exists in database",
                        "severity": "warn",
                        "summary": "Unable to save changes",
                        "type": "notify"
                      },
                      {
                        "condition": "${bDuplicateName}",
                        "detail": "Brokerage Name already exists in the database",
                        "severity": "warn",
                        "summary": "Unable to Save Changes",
                        "type": "notify"
                      }
                    ]
                  },
                  "itemType": "RecoCms6.Models.RecoDb.Brokerage",
                  "name": "form0",
                  "type": "template-form",
                  "visible": "${brokerage != null}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
