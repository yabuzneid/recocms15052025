      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "error": [],
        "name": "RECODb.getBrokerageByBrokerageID",
        "parameters": [
          {
            "name": "BrokerageID",
            "value": "${parameters.BrokerageID}"
          }
        ],
        "then": [
          {
            "name": "brokerage",
            "type": "set",
            "value": "${result}",
            "valueType": "RecoCms6.Models.RecoDb.Brokerage"
          },
          {
            "name": "origBrokerage",
            "type": "set",
            "value": "${result}",
            "valueType": "RecoCms6.Models.RecoDb.Brokerage"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "RECODb.getRegistrants",
        "parameters": [],
        "then": [
          {
            "name": "getRegistrantsResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RecoCms6.Models.RecoDb.Registrant>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "RECODb.getParameterDetails",
        "parameters": [
          {
            "name": "$filter",
            "operator": "or",
            "value": [
              {
                "operation": "eq",
                "property": "ParamTypeDesc",
                "type": "string",
                "value": "\"Province\""
              },
              {
                "operation": "eq",
                "property": "ParamTypeDesc",
                "type": "string",
                "value": "\"Method of Communication\""
              },
              {
                "operation": "eq",
                "property": "ParamTypeDesc",
                "type": "string",
                "value": "\"Brokerage Role\""
              }
            ]
          },
          {
            "name": "$orderby",
            "value": [
              {
                "direction": "asc",
                "property": "ParamDesc"
              }
            ]
          }
        ],
        "then": [
          {
            "name": "getProvinceList",
            "type": "set",
            "value": "${result.Where(pd=>pd.ParamTypeDesc==\"Province\")}",
            "valueType": "IEnumerable<RecoCms6.Models.RecoDb.ParameterDetail>"
          },
          {
            "name": "getCommunicationMethodList",
            "type": "set",
            "value": "${result.Where(pd=>pd.ParamTypeDesc==\"Method of Communication\")}",
            "valueType": "IEnumerable<RecoCms6.Models.RecoDb.ParameterDetail>"
          },
          {
            "name": "getBrokerageRoleList",
            "type": "set",
            "value": "${result.Where(p=>p.ParamTypeDesc==\"Brokerage Role\")}",
            "valueType": "dynamic"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "RECODb.getAdministrators",
        "parameters": [],
        "then": [
          {
            "name": "getAdministratorsResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RecoCms6.Models.RecoDb.Administrator>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "bRegNumberChanged",
